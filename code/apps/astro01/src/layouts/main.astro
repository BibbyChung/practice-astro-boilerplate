---
import Layout from '~/layouts/layout.astro'
import Menu from '~/lib/components/menu.astro'

type Props = {
  title: string
}
const props = Astro.props
---

<Layout {...props}>
  <main class="p-8">
    <div class="w-full">
      <Menu />
    </div>
    <div class="mt-12 flex w-full flex-col items-start">
      <slot />
    </div>
  </main>
</Layout>
